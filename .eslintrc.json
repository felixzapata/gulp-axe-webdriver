{
  "extends": "eslint:recommended",
    "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "off"
  },
  "env": {
    "node": true,
    "browser": false
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "require" : false,
    "__dirname" : false,
    "describe" : false,
    "process" : false,
    "beforeEach" : false,
    "afterEach" : false,
    "module" : false,
    "it" : false,
    "xit": false,
    "xdescribe": false
  }
}
